{
   "manifest_version": 2,
   "background": {
      "scripts": ["background.js"]
   },
   "content_scripts": [ {
      "exclude_globs": [  ],
      "exclude_matches": [  ],
      "include_globs": [  ],
      "js": [ "jquery.js", "script.js" ],
      "matches": [ "http://www.netflix.com/WiPlayer*" ],
      "run_at": "document_idle"
   } ],
   "icons": {
           "48": "netflix48.png",
          "128": "netflix128.png" },
   "description": "Allows one to control a netflix watch page with an xbox (only tested) controller.",
   "name": "Netflix Controller Support",
   "version": "0.1",
   "permissions": [
      "tabs"
   ]
}